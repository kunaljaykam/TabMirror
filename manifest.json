{
  "manifest_version": 3,
  "name": "TabMirror",
  "version": "1.0",

  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "permissions": ["tabs"],

  "icons": {
    "16": "tab-mirror.png",
    "48": "tab-mirror.png",
    "128": "tab-mirror.png"
  }
  
}
